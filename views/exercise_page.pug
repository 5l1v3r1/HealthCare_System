doctype html
html(lang='en')
  head
    // Bootstrap core JavaScript
    script(src='../src/vendor/jquery/jquery.min.js')
    script(src='../src/vendor/bootstrap/js/bootstrap.bundle.min.js')
    // Core plugin JavaScript
    script(src='../src/vendor/jquery-easing/jquery.easing.min.js')
    // Page level plugin JavaScript
    script(src='../src/vendor/chart.js/Chart.min.js')
    script(src='../src/vendor/datatables/jquery.dataTables.js')
    script(src='../src/vendor/datatables/dataTables.bootstrap4.js')
    // Custom scripts for all pages
    script(src='../src/js/sb-admin.min.js')
    // Demo scripts for this page
    script(src='../src/js/demo/datatables-demo.js')
    script(src='../src/js/demo/chart-area-demo.js')
    meta(charset='utf-8')
    script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js")
    script(src="../src/fun.js")
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    meta(name='description', content='')
    meta(name='author', content='')
    title Health Care Admin Console
    // Bootstrap core CSS
    link(href='../src/vendor/bootstrap/css/bootstrap.min.css', rel='stylesheet')
    // Custom fonts for this template
    link(href='../src/vendor/fontawesome-free/css/all.min.css', rel='stylesheet', type='text/css')
    // Page level plugin CSS
    link(href='../src/vendor/datatables/dataTables.bootstrap4.css', rel='stylesheet')
    // Custom styles for this template
    link(href='../src/css/sb-admin.css', rel='stylesheet')
    // Custom upload template
    link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css', integrity='sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T', crossorigin='anonymous')

  body#page-top
    nav.navbar.navbar-expand.navbar-dark.bg-dark.static-top
      a.navbar-brand.mr-1(href='/HealthCare/main') Health Care Admin Console
      button#sidebarToggle.btn.btn-link.btn-sm.text-white.order-1.order-sm-0(href='#')
        i.fas.fa-bars
    #wrapper
      // Sidebar
      ul.sidebar.navbar-nav
        li.nav-item.active
          a.nav-link(href='/HealthCare/main')
            i.fas.fa-fw.fa-chart-area
            span Home
        // Content no list mode
          li.nav-item
            a.nav-link(href='/HealthCare/category_page')
              i.fas.fa-fw.fa-tachometer-alt
              span Category
          li.nav-item
            a.nav-link(href='/HealthCare/exercise_page')
              i.fas.fa-fw.fa-table
              span Exercise
        // Content list mode 
        li.nav-item.dropdown
          a#pagesDropdown.nav-link.dropdown-toggle(href='#', role='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
            i.fas.fa-fw.fa-folder
            span Services
          .dropdown-menu(aria-labelledby='pagesDropdown')
            // h6.dropdown-header Login Screens:
            a.dropdown-item(href='/HealthCare/category_page') Category
            a.dropdown-item(href='/HealthCAre/exercise_page') Exercise
            // divide line
              .dropdown-divider
              h6.dropdown-header Other Pages:
              a.dropdown-item(href='404.html') 404 Page
              a.dropdown-item(href='blank.html') Blank Page
      #content-wrapper
        .container-fluid
          // Breadcrumbs
          ol.breadcrumb
            li.breadcrumb-item.active Exercise & Thumbnail Upload Delete Update
        // Body
        .container-fluid
          .exercise-upload-box
            p.h2 Exercise & Thumbnail Upload
            p
             select#category_box
              option(vaule='null', selected='') --choice--
              each element in categorylist
               option(value=element) #{element}
             p.h5 Exercise :
             input(type='text',id='exercisebox' name='exercisebox')
             p.h5 Exercise Description
             textarea(id='exercisedescbox' ,name='exercisedescbox', cols='40', rows="5")
             //form(action='/HealthCare_API/upload', method='POST', enctype='multipart/form-data')
             table(width='400', cellpadding='5', cellspacing='2', border='0', align='')
              tr
               td
                .custom-file.mb-3
                 input#file.custom-file-input(type='file',name='file')
                 label.custom-file-label(for='file' ,width=200) Choose File
                 //input.btn.btn-primary.btn-block(type='submit',width=200 value='Submit')
             button.btn.btn-primary(type='button', name='submit_upload',onclick='postExercise()', value='Submit') Submit
             br     
          .science-app-name-box
            .card.mb-3
            p.h2 Science App Name Box
            br
            p
             select#scienceappname_box
              option(vaule='null', selected='') --choice--
             button.btn.btn-primary(type='button', name='next2', onclick='getResult_statistics()', value='Next') Next
             button.btn.btn-primary(type='button', name='back2', onclick='reset_parameter_table()', value='Back') Back
             br
             br
          p
            .card.mb-3
            .card-header
             i.fas.fa-table
              |               Parameter Rank Top  
              br
              br
              select#indexcount_box.custom-select.custom-select-sm.form-control.form-control-sm(name='dataTable_length',onchange='getResult_statistics()', aria-controls='dataTable')
                option(value='5') 5
                option(value='10', selected='') 10
                option(value='25') 25
                option(value='50') 50
                option(value='100') 100
            .parameter-table
             .table-responsive
              table#parameter_table.table.table-bordered(width='10%', cellspacing='0')
               thead
               tbody
            .reset-button
             button.btn.btn-primary(type='button', name='reset', onclick='reset_all()', value='Reset') Reset
        // Sticky Footer
        footer.sticky-footer
          .container.my-auto
            .copyright.text-center.my-auto
              span Copyright Â© Hyun Tae Jeong
   
